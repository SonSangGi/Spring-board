<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings><setting name="jdbcTypeForNull" value="NULL"/></settings>
    <typeAliases>
        <typeAlias type="com.mqnic.board.domain.BoardVO" alias="Board"/>
        <typeAlias type="com.mqnic.board.domain.BoardAttachVO" alias="BoardAttach"/>
        <typeAlias type="com.mqnic.board.domain.ReplyVO" alias="Reply"/>
        <typeAlias type="com.mqnic.board.domain.AuthVO" alias="Auth"/>
        <typeAlias type="com.mqnic.board.domain.MemberVO" alias="Member"/>
    </typeAliases>
    <mappers>
        <mapper resource="META-INF/mybatis/mappers/BoardMapper.xml"/>
        <mapper resource="META-INF/mybatis/mappers/ReplyMapper.xml"/>
        <mapper resource="META-INF/mybatis/mappers/BoardAttachMapper.xml"/>
        <mapper resource="META-INF/mybatis/mappers/MemberMapper.xml"/>
    </mappers>
</configuration>